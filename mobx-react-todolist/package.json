{
  "name": "mobx-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^10.2.6",
    "customize-cra": "^1.0.0",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "tailwindcss": "^2.1.4",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm run bulid:css && react-app-rewired start",
    "build": "npm run build:css && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "bulid:css": "postcss src/style/tailwind.css -o src/style/main.css",
    "build:watch": "postcss src/styles/tailwind.css -o src/styles/main.css --watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
